generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Company {
  id                 Int     @id @default(autoincrement())
  title              String
  purpose            String?
  website            String?
  phone              String?
  email              String?
  city               String?
  canton             String?
  category           String?
  geoMetrics         Json?
  seoMetrics         Json?
  performanceMetrics Json?
  validationMetrics  Json?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
